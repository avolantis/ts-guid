{
  "name": "@avolantis/ts-guid",
  "version": "0.1.0-preview.0",
  "keywords": [
    "guid",
    "uuid",
    "unique",
    "uniqueidentifier",
    "id",
    "identifier",
    "uuid-generator"
  ],
  "description": "TypeScript implementation of GUIDs (v4 UUIDs)",
  "homepage": "https://github.com/avolantis/ts-guid#readme",
  "repository": "https://github.com/avolantis/ts-guid",
  "bugs": "https://github.com/avolantis/ts-guid/issues",
  "author": "Avolantis <info@avolantis.net> (https://avolantis.net)",
  "license": "MIT",
  "main": "dist/ts-guid.cjs.js",
  "module": "dist/ts-guid.esm.min.js",
  "browser": "dist/ts-guid.iife.min.js",
  "unpkg": "dist/ts-guid.umd.min.js",
  "types": "types/ts-guid.d.ts",
  "files": [
    "dist",
    "lib",
    "src",
    "types"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c ./config/rollup.js",
    "build:ci": "cross-env CI=true rollup -c ./config/rollup.js",
    "changelog": "conventional-changelog -i ./CHANGELOG.md -s -n ./config/changelog.js",
    "ci": "yarn install --frozen-lockfile --non-interactive",
    "clean": "rimraf dist lib types",
    "clean:deps": "rimraf node_modules",
    "dev": "tsc",
    "lint": "yarn lint:files .",
    "lint:files": "eslint --fix",
    "release": "standard-version",
    "test": "yarn test:node && yarn test:jsdom",
    "test:node": "jest -c config/jest-node.js",
    "test:jsdom": "jest -c config/jest-jsdom.js",
    "test:ci": "cross-env CI=true yarn lint && yarn test:coverage",
    "test:coverage": "yarn test:node --coverage && yarn test:jsdom"
  },
  "gitHooks": {
    "pre-commit": "lint-staged -c ./config/lint-staged.js",
    "commit-msg": "commitlint -g ./config/commitlint.js -E GIT_PARAMS"
  },
  "prettier": "./config/prettier.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "commitlint": "^11.0.0",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-package-json": "^0.1.4",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-dts": "^2.0.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "yorkie": "^2.0.0"
  }
}
